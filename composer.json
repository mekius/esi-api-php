{
    "name": "mekius/esi-api-php",
    "description": "EVE Online API Library for PHP",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "eve",
        "esi"
    ],
    "homepage": "http://swagger.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "ESI\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "ESI\\" : "test/" }
    }
}
